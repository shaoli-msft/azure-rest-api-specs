{
  "title": "Train LargePersonGroup",
  "operationId": "PersonGroupOperations_TrainLargePersonGroup",
  "parameters": {
    "apiVersion": "v1.1-preview.1",
    "largePersonGroupId": "your_large_person_group_id"
  },
  "responses": {
    "202": {
      "headers": {
        "operation-Location": "https://contoso.com/operationstatus"
      }
    },
    "401": {
      "body": {
        "error": {
          "code": "401",
          "message": "Access denied due to invalid subscription key or wrong API endpoint. Make sure to provide a valid key for an active subscription and use a correct regional API endpoint for your resource."
        }
      }
    },
    "404": {
      "body": {
        "error": {
          "code": "LargePersonGroupNotFound",
          "message": "Large Person group is not found."
        }
      }
    },
    "409": {
      "body": {
        "error": {
          "code": "PersonGroupTrainingNotFinished",
          "message": "Person group is under training."
        }
      }
    },
    "429": {
      "body": {
        "error": {
          "code": "429",
          "message": "Rate limit is exceeded."
        }
      }
    }
  }
